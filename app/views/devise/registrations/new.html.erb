<div class="container">
</div>
    <%= render "devise/shared/links" %>
<div id="sign">
<div class="container-devise">
  <div class="col-md-6">
    <h2>Sign Up</h2>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>

    <div class="form-group">
      <label for="formGroupEcampleInput"> <%= f.label :email %> </label>
      <input type="text" class="rounded-pill-dev" id="formGroupEcampleInput" placeholder="email@email.com" <%= f.email_field :email, autofocus: true, autocomplete: "email" %> </input>
    </div>

    <div class="form-group">
      <label for="formGroupEcampleInput2"> <%= f.label :password %> </label>
      <% if @minimum_password_length %>
      <em>(<%= @minimum_password_length %> characters minimum)</em>
      <% end %><br />
      <input type="password" class="rounded-pill-dev" id="inputPassword" placeholder="password" <%= f.password_field :password, autocomplete: "new-password" %> </input>
    </div>

    <div class="form-group">
      <label for="formGroupExampleInput2">Password Confirmation</label>
      <input type="password" id="inputPassword" class="rounded-pill-dev" placeholder="password" required="" <%= f.password_field :password_confirmation, autocomplete: "new-password" %></input>
    </div>

    <div class="actions">
      <input type="submit"class="btn btn-primary rounded-pill-dev" value="Sign up"<%= f.submit "Sign up" %>
      </div>
    <% end %>
      </div>
</div>