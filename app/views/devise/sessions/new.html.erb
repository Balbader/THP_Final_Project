<div class="container login-container d-flex justify-content-center">
  <div class="col-md-6 login-form-1">
    <h2>Sign In</h2>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

    <div class="form-group">
      <label for="formGroupEcampleInput"> <%= f.label :email %> </label>
      <input type="text" class="form-control" id="formGroupEcampleInput" placeholder="email@email.com" <%= f.email_field :email, autofocus: true, autocomplete: "email" %> </input>
    </div>

    <div class="form-group">
      <label for="formGroupExampleInput2">Password</label>
      <input type="password" id="inputPassword" class="form-control" placeholder="password" required="" <%= f.password_field :password, autocomplete: "current-password" %>
    </div>

    <div class="actions">
      <input type="submit" class="btn btn-lg btn-primary btn-block" value="Sign in"<%= f.submit "Sign in" %>
    </div>
    <% end %>
    <%= render "devise/shared/links" %>
  </div>
</div>
